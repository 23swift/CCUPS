{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0-rc.0",
    "@material-ui/icons": "^4.0.0",
    "@material-ui/styles": "^4.0.0",
    "bootstrap": "^4.3.1",
    "formik": "^1.5.7",
    "formik-material-ui": "0.0.18",
    "material-table": "^1.39.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-sortablejs": "^1.5.1",
    "react-transition-group": "^4.1.1",
    "sortablejs": "^1.6.1",
    "yup": "^0.27.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./node_modules/bootstrap/scss -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar ./src -o src/styles --watch --recursive",
    "start": "react-scripts start  && npm run watch-css ",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy-to-backend": "gulp copy"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "node-sass-chokidar": "^1.3.5"
  }
}
